{
  "name": "frontend-standard-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "dotenv -e .env.local vite",
    "start:dev": "dotenv -e .env.development vite",
    "build": "tsc -b && vite build",
    "lint": "npx biome lint --diagnostic-level=error ./src",
    "lint:fix": "npx biome lint --fix ./src",
    "preview": "vite preview",
    "format": "biome format --write ./",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage",
    "test:basic": "vitest basic run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@reduxjs/toolkit": "^2.2.6",
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.4",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.1.2",
    "jsdom": "^24.1.1",
    "msw": "^2.3.4",
    "pinst": "^3.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vitest": "^2.0.4"
  }
}
